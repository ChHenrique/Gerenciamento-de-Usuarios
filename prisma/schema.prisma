generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid()) // PostgreSQL precisa de um ID Ãºnico gerado
  name      String
  photoUrl  String
  email     String @unique
  password  String
}
